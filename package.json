{
  "name": "awesome-remark",
  "private": true,
  "version": "0.0.0",
  "description": "Curated list of awesome remark resources",
  "license": "CC-BY-4.0",
  "keywords": [],
  "repository": "remarkjs/awesome-remark",
  "bugs": "https://github.com/remarkjs/awesome-remark/issues",
  "author": "Titus Wormer <tituswormer@gmail.com> (wooorm.com)",
  "contributors": [
    "Titus Wormer <tituswormer@gmail.com> (wooorm.com)"
  ],
  "devDependencies": {
    "awesome-lint": "^0.10.0",
    "remark-cli": "^7.0.0",
    "remark-preset-wooorm": "^6.0.0"
  },
  "scripts": {
    "format": "remark . -qo",
    "test": "npm run format"
  },
  "remarkConfig": {
    "settings": {
      "listItemIndent": "1"
    },
    "plugins": [
      "preset-wooorm",
      [
        "lint-list-item-indent",
        "space"
      ],
      [
        "toc",
        {
          "heading": "contents"
        }
      ],
      "awesome-lint/config",
      [
        "awesome-lint/rules/contributing.js",
        false
      ]
    ]
  }
}
